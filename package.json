  },
  "description": "![License: MIT](https://img.shields.io/badge/License-MIT-yellow.svg)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beratcode03/beratders.git"
  },
  "keywords": [
    "yks",
    "egitim",
    "analiz",
    "takip"
  ],
  "author": "Berat CankÄ±r",
  "bugs": {
    "url": "https://github.com/beratcode03/beratders/issues"
  },
  "homepage": "https://github.com/beratcode03/beratders#readme",
  "build": {
    "asar": true,
    "appId": "com.beratcankir.yksanaliz",
    "productName": "Berat Cankir",
    "executableName": "BeratCankir",
    "npmRebuild": true,
    "buildDependenciesFromSource": false,
    "nodeGypRebuild": false,
    "publish": {
      "provider": "github",
      "owner": "beratcode03",
      "repo": "beratders",
      "releaseType": "release"
    },
    "directories": {
      "output": "dist-electron"
    },
    "files": [
      "electron/**/*",
      "dist/**/*",
      "package.json",
      ".env",
      "node_modules/**/*",
      "!node_modules/sharp/**/*",
      "!**/*.map",
      "!**/*.md",
      "!**/LICENSE",
      "!**/README"
    ],
    "extraResources": [
      {
        "from": "client/public/ataturk.png",
        "to": "assets/ataturk.png"
      },
      {
        "from": "client/public/ataturkimza.png",
        "to": "assets/ataturkimza.png"
      },
      {
        "from": "client/public/turkbayragi.png",
        "to": "assets/turkbayragi.png"
      }
    ],
    "asarUnpack": [
      "dist/**/*",
      "electron/icons/**/*"
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "electron/icons/app-icon.ico",
      "verifyUpdateCodeSignature": false
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": "always",
      "createStartMenuShortcut": true,
      "runAfterFinish": true,
      "installerIcon": "electron/icons/app-icon.ico",
      "uninstallerIcon": "electron/icons/app-icon.ico",
      "installerHeaderIcon": "electron/icons/app-icon.ico",
      "deleteAppDataOnUninstall": false,
      "perMachine": false,
      "allowElevation": true,
      "displayLanguageSelector": false,
      "menuCategory": "Berat Cankir",
      "shortcutName": "Berat Cankir",
      "artifactName": "${productName}-Kurulum-${version}.${ext}",
      "license": "LICENSE.txt",
      "warningsAsErrors": false,
      "language": 1055,
      "multiLanguageInstaller": false,
      "differentialPackage": false
    }
  }
}
