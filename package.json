{
  "name": "beratcankir",
  "version": "0.0.3",
  "type": "module",
  "license": "MIT",
  "main": "electron/main.cjs",
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx server/index.ts",
    "build": "vite build && npm run build-server",
    "build-server": "esbuild server/index.ts --platform=node --bundle --external:lightningcss --format=esm --outdir=server/dist",
    "build-electron": "vite build && npm run build-server-electron",
    "build-server-electron": "esbuild server/index.ts --platform=node --bundle --external:lightningcss --external:vite --external:@vitejs/* --external:rollup --external:@rollup/* --format=cjs --outfile=dist/index.cjs",
    "start": "cross-env NODE_ENV=production node server/dist/index.js",
    "electron": "electron electron/main.cjs",
    "electron:dev": "cross-env NODE_ENV=development electron .",
    "electron:build": "npm run preelectron-build && electron-builder build --win --x64",
    "electron:build:dir": "npm run preelectron-build && electron-builder build --win --x64 --dir",
    "preelectron-build": "npm run build-electron",
    "electron-build": "electron-builder",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
